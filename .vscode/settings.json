{
  "eslint.enable": true,
  "eslint.validate": ["javascript", "javascriptreact"],
  "eslint.autoFixOnSave": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.rulers": [88]
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.rulers": [88]
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "javascript.validate.enable": false
}
